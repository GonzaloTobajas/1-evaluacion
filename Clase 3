#Bucles: llamar un número de teléfono
import time
print("¿A quien deseas llamar?")
def bucle_1():
    suma="Llamando a " #es una variable acumuladora
    for cont in range(1,10):
        num=input("Dime un numero: ")
        suma=suma+num
    print(suma)
bucle_1()
print("...")
time.sleep(1)
print("...")
time.sleep(1)
print("...")
time.sleep(1)
print("El número al que has llamado se encuentra fuera de covertura")
